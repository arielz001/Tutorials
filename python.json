{
    "print f newline": {
        "prefix": "pf",
        "body": ["print(f\"\\n$1\\n\")"],
        "description": "print f with newline"
    },

    "debug print": {
        "prefix": "dbg",
        "body": ["print(f\"\\nDEBUG: {$1}\\n\")"],
        "description": "debug print"
    },

    "if": {
        "prefix": "if",
        "body": ["if $1:", "    $2"],
        "description": "if statement"
    },

    "if else": {
        "prefix": "ife",
        "body": ["if $1:", "    $2", "else:", "    $3"],
        "description": "if else"
    },

    "for": {
        "prefix": "for",
        "body": ["for $1 in $2:", "    $3"],
        "description": "for loop"
    },

    "for range": {
        "prefix": "fori",
        "body": ["for i in range($1):", "    $2"],
        "description": "for i in range"
    },

    "while": {
        "prefix": "while",
        "body": ["while $1:", "    $2"],
        "description": "while loop"
    },

    "def": {
        "prefix": "def",
        "body": ["def $1($2):", "    $3"],
        "description": "function"
    },

    "class": {
        "prefix": "class",
        "body": [
            "class $1:",
            "    def __init__(self, $2):",
            "        $3"
        ],
        "description": "class"
    },

    "main": {
        "prefix": "main",
        "body": ["if __name__ == \"__main__\":", "    $1"],
        "description": "main guard"
    },

    "try except": {
        "prefix": "try",
        "body": ["try:", "    $1", "except Exception as e:", "    $2"],
        "description": "try except"
    },

    "try except finally": {
        "prefix": "tryf",
        "body": [
            "try:",
            "    $1",
            "except Exception as e:",
            "    $2",
            "finally:",
            "    $3"
        ],
        "description": "try except finally"
    },

    "with": {
        "prefix": "with",
        "body": ["with $1 as $2:", "    $3"],
        "description": "with statement"
    },

    "assert": {
        "prefix": "ass",
        "body": ["assert $1, \"$2\""],
        "description": "assert"
    },

    "numpy import": {
        "prefix": "np",
        "body": ["import numpy as np"],
        "description": "numpy"
    },

    "matplotlib import": {
        "prefix": "plt",
        "body": ["import matplotlib.pyplot as plt"],
        "description": "matplotlib"
    },

    "opencv import": {
        "prefix": "cv",
        "body": ["import cv2"],
        "description": "opencv"
    },

    "torch import": {
        "prefix": "torch",
        "body": [
            "import torch",
            "import torch.nn as nn",
            "import torch.nn.functional as F"
        ],
        "description": "torch"
    },

    "torch no grad": {
        "prefix": "nograd",
        "body": ["with torch.no_grad():", "    $1"],
        "description": "torch no grad"
    },

    "dataset class": {
        "prefix": "dataset",
        "body": [
            "class $1(torch.utils.data.Dataset):",
            "    def __init__(self, $2):",
            "        $3",
            "",
            "    def __len__(self):",
            "        return $4",
            "",
            "    def __getitem__(self, idx):",
            "        return $5"
        ],
        "description": "torch dataset"
    },

    "ros node": {
        "prefix": "rosnode",
        "body": [
            "import rclpy",
            "from rclpy.node import Node",
            "",
            "class $1(Node):",
            "    def __init__(self):",
            "        super().__init__(\"$2\")",
            "        $3",
            "",
            "def main(args=None):",
            "    rclpy.init(args=args)",
            "    node = $1()",
            "    rclpy.spin(node)",
            "    node.destroy_node()",
            "    rclpy.shutdown()",
            "",
            "if __name__ == \"__main__\":",
            "    main()"
        ],
        "description": "ros2 node"
    },

    "timer": {
        "prefix": "timer",
        "body": [
            "import time",
            "t0 = time.time()",
            "$1",
            "print(f\"\\nTime: {time.time() - t0:.3f}s\\n\")"
        ],
        "description": "timer"
    }
}
